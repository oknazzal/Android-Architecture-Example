<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oknazzal.architecture.example.feature.images.presentation" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="33" />

    <application>
        <activity
            android:name="com.oknazzal.architecture.example.feature.images.presentation.list.ImagesListActivity"
            android:exported="false" >
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />

                <action android:name="com.oknazzal.images.LIST" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.oknazzal.architecture.example.feature.images.presentation.details.ImageDetailsActivity"
            android:exported="false" >
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />

                <action android:name="com.oknazzal.images.DETAILS" />
            </intent-filter>
        </activity>
    </application>

</manifest>